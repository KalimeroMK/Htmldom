{
    "name": "kalimeromk/update-htmldom",
    "type": "library",
    "description": "Simple Html Dom Parser for Laravel",
    "keywords": ["laravel", "html parser"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zoran",
            "email": "zbogoevski@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^7.4|^8.0|^8.1",
        "ext-iconv": "*"
    },
    "autoload": {
        "psr-0": {
            "Htmldom": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Htmldom\\HtmldomServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
